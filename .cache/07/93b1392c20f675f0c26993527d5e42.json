-family: sans-serif;\n  justify-content: center;\n  text-align: center;\n}\n\n.button {\n  display: block;\n  font-size: $input-size;\n  margin: 0.5rem;\n  padding: 0.5rem;\n  width: 10rem;\n  border-radius: 1rem;\n\n  &--inc {\n    background-color: lightblue;\n  }\n\n  &--dec {\n    background-color: pink;\n  }\n}\n.input_box {\n  font-size: 2.5rem;\n  height: 3rem;\n  width: 8rem;\n  text-align: center;\n  flex-basis: 65%;\n}\n\n.slider {\n  font-size: 3rem;\n  height: 3rem;\n}"},"lineCount":null}},"error":null,"hash":"8f47c7e990f4c8c0b9c1e258a8de6b36","cacheData":{"env":{}}}